{
  "title": "Yolo Image Annotator",
  "description": "A totally free, local and lightweight object detector trained on 80 different types of objects.",
  "version": "1.0.2.0",
  "author": "Monkey",
  "guid": "a54545be-e64e-4f51-a2fe-9206d5c64496",
  "readmeUrl": "https://raw.githubusercontent.com/monkeypaste/YoloImageAnnotator/master/README.md",
  "projectUrl": "https://github.com/monkeypaste/YoloImageAnnotator",
  "packageUrl": "https://github.com/monkeypaste/YoloImageAnnotator/releases/download/v1.0.2.0/v1.0.2.0.zip",
  "iconUri": "https://raw.githubusercontent.com/monkeypaste/YoloImageAnnotator/master/icon.png",
  "packageType": "Dll",
  "tags": "Image Detection, Annotation, Image, AI, Text",
  "analyzer": {
    "inputType": {
      "image": true
    },
    "outputType": {
      "imageToken": true
    },
    "parameters": [
      {
        "label": "Confidence",
        "description": "A value between 0 and 1 to filter detected accuracy",
        "controlType": "Slider",
        "unitType": "Decimal",
        "minimum": 0,
        "maximum": 1,
        "isRequired": true,
        "paramId": "conf",
        "values": [
          {
            "value": "0.1",
            "isDefault": true
          }
        ]
      },
      {
        "label": "Source Content",
        "description": "The base64-encoded image to analyze",
        "controlType": "TextBox",
        "unitType": "PlainTextContentQuery",
        "isRequired": true,
        "isVisible": false,
        "paramId": "img64",
        "values": [
          {
            "value": "{ClipText}"
          }
        ]
      }
    ]
  }
}